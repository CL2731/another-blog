<h2 class="center-align blue-text text-darken-2">Sign Up!</h2>
<div class="row container">
    <form id="signUp" class="col s12">
        <div class="row container col s8 offset-s2">
            <div class="input-field">
                <input id="name" type="text" class="validate" />
                <label for="name">Name</label>
            </div>
        </div>
        <div class="row container col s8 offset-s2">
            <div class="input-field">
                <input id="signUpPassword" type="password" class="validate" />
                <label for="password">Password</label>
                <div class="row container center-align">
                    <button class="btn btn-light" type="submit">Enter
                    </button>

                </div>
    </form>
</div>
<script src="js/signup.js"></script>